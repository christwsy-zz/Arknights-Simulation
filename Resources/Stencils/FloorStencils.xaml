            <c:Floor IsHitTestVisible="False" FloorType="EnemyExit" />
            <c:Floor IsHitTestVisible="False" FloorType="Pithole" />
            <c:Floor IsHitTestVisible="False" FloorType="MedicalRune" />
            <c:Floor IsHitTestVisible="False" FloorType="AntiAirRune" />
            <c:Floor IsHitTestVisible="False" FloorType="ProtectRune" />
            <c:Floor IsHitTestVisible="False" FloorType="Grass" />
            <c:Floor IsHitTestVisible="False" FloorType="ScoutTower" />
            <c:Floor IsHitTestVisible="False" FloorType="EMP" />
            <c:Floor IsHitTestVisible="False" FloorType="ActivatedOriginStone" />
            <c:Floor IsHitTestVisible="False" FloorType="HeatPump" />
        </ItemsControl.Items>
    </s:Toolbox>
</ResourceDictionary>